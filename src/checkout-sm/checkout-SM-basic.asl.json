                                        "account.$": "$.userId"
                                    },
                                    "connectionArn.$": "$.connectionArn",
                                    "operation": "GET"
                                }
                            },
                            "ResultSelector": {
                                "ResponseBody.$": "$.Output.ResponseBody"
                            },
                            "ResultPath": "$.UserInfo",
                            "Next": "FilterUserInfo"
                        },
                        "FilterUserInfo": {
                            "Type": "Pass",
                            "Parameters": {
                                "UserInfo.$": "$.UserInfo"
                            },
                            "End": true
